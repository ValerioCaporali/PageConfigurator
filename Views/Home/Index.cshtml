@{
    ViewData["Title"] = "Home Page";
}



<!DOCTYPE html>
<html lang="it">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <script src='https://kit.fontawesome.com/a076d05399.js' crossorigin='anonymous'></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jsoneditor/9.7.4/jsoneditor.css" integrity="sha512-XmjnlPNi5tkrcqMGEG7ZOpGhny9t8fdCk0oxZxjFaqF3cl3UKv+51GgeUoY3Rf/ZaGtBvMZ9dDhceGZ1hcOyMw==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css"
        integrity="sha512-9usAa10IRO0HhonpyAIVpjrylPvoDwiPUiKdWk5t3PyolY1cOd4DSE0Ga+ri4AuTroPR5aQvXU9xC6qOPnzFeg=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link href="~/css/Sidebar.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />
    <link href="~/css/configurator-style.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="https://cdn3.devexpress.com/jslib/21.2.4/css/dx.common.css" />
    <link rel="stylesheet" type="text/css" href="https://cdn3.devexpress.com/jslib/21.2.4/css/dx.light.css" />
    <script src="https://cdn.tiny.cloud/1/tanbthyu2xdpiwjwhtvcw768cj6626e06m1hl4qo7m96fyvc/tinymce/5/tinymce.min.js" referrerpolicy="origin"></script>
    <script src="https://cdn.tiny.cloud/1/tanbthyu2xdpiwjwhtvcw768cj6626e06m1hl4qo7m96fyvc/tinymce/5/jquery.tinymce.min.js" referrerpolicy="origin"></script>
</head>

<style>
    [contenteditable="true"]:active,
    [contenteditable="true"]:focus {
        border-bottom: 1px solid black;
        padding: 2px;
        outline: none;
    }

    [contenteditable="true"]:focus>style::before {
        content: "styles";
        font-size: 1.5em;
        font-weight: 700;
    }

    [contenteditable="true"]:focus>style {
        display: block;
        position: relative;
        right: -50%;
        top: -2em;
        color: blue;
        font-family: courier, monospace;
        font-size: .75em;
        line-height: 1.5;
        padding: .5em;
        background: #fff;
        border: 1px solid #999;
        box-shadow: 1px 1px 2px #777;
        border-radius: 5px;
    }

    #navbar {
        position: fixed;
        width: 100%;
        top: 0;
        z-index: 1000;
    }
    
    ul {
        list-style-type: none;
        padding: 0;
    }
    
    #list {
        padding: 0;
    }
    
    #home-pages-list-container ul li,
    #pages-list-container ul li {
        background-color: #ededed;
        padding: 13px;
        margin: 13px 0px;
        border: 1px solid #999999;
        border-radius: 3px;
        transition: all 0.3s;
    }
    
    #home-pages-list-container ul li img,
    #pages-list-container ul li img{
        cursor: pointer;
    }
    
    #grid-page-view, #list-page-view {
        cursor: pointer;
    }
    
    #add-icon:hover {
        cursor:pointer;
    }
    
    .presets {
        justify-content: center;
        margin: 0 auto;
    }
    
    .presets-label {
        justify-content: center;
        margin: 0;
        margin-bottom: 30px !important;
    }
    
    .presets-label p {
        padding: 0 !important;
        font-size: 16px !important;
    }

    .resizer {
        margin-top: -20px !important;
        opacity: 0;
        display: none;
        width: 20px;
        height: 20px;
        background: #000;
        margin: auto;
        float: right;
        cursor: se-resize;
        display: none;
    }
    
    .preset {
        padding: 0px 20px;
        opacity: 60%;
        transition: all 0.1s;
    }
    
    .preset:hover {
        cursor: pointer;
        opacity: 100%;
    }

     #sidebar {
        position: fixed;
        width: 18%;
        top: 0%;
        right: 0;
        bottom: 0;
        background-color: #e6e9ec!important;
        font-family: Roboto,Arial,Helvetica,Verdana,sans-serif;
        font-size: 13px;
        z-index: 1;
        box-shadow: 14px 0px 15px 6px #000000;
    }

    .sidebar-header {
        padding: 15px;
    }

    .sidebar-label {
        margin: auto;
        margin-left: 0;
        color: #556068;
        font-size: 10.4px;
    }

    .sidebar-icon {
        vertical-align: middle;
        margin: auto;
        margin-right: 0;
    }

    .sidebar-footer {
        bottom: 0;
        width: 100%;
        padding: 10px 3px;
        background: #e6e9ec !important;
        box-shadow: 0px -1px 5px #d8d9db !important;
        border-top: none !important;
    }

    #delete-widget-btn {
        margin: auto;
        margin-right: 10px;
    }

    .sidenav {
        width: 100%;
        margin: 0;
        padding: 0;
        display: flex;
    }

    .dx-button-has-icon .dx-icon {
        width: 25px;
        height: 25px;
    }

    #page-title {
        display: none;
        margin-left: 0 !important;
        margin: auto;
        color: #9a9a9a;
    }

    #save-draft-button, #delete-draft-button, #publish-button, #delete-page-button {
        margin-right: 6px;
    }

    #sidebar-default-view {
        max-width: 100% !important;
        margin-top: 20px;
        padding: 0 !important;
    }

    #metadata {
        height: 250px;
        transition: height 0.2s;
        margin: 4px;
        padding: 5px;
    }

    #events-curtain, #text-curtain, #content-curtain {
        transition: max-height 0.3s;
    }

    #widgets-curtain-header:hover, 
    #metadata-curtain-header:hover,
    #text-curtain-header:hover, 
    #content-curtain-header:hover, 
    #style-curtain-header,
    #mobile-style-curtain-header,
    #events-curtain-header,
    #widgets-list-button {
        cursor: pointer;
    }
    
    #metadata-curtain-header,
    #text-curtain-header,
    #events-curtain-header,
    #mobile-style-curtain-header {
        box-shadow: 0px 8px 4px -2px rgb(0 0 0 / 3%);
    }
    
    #events-curtain  {
        height: 0px;
    }

    #widgets-curtain, #metadata,
    #text-curtain,
    #content-curtain,
    #style-curtain,
    #mobile-style-curtain,
    #events-curtain {
        padding: 0px 16px;
    }

    #text-curtain {
        height: auto;
        transition: all 0.3ms;
    }

    .curtain-header-shadow {
        transition: all 0.3ms;
        box-shadow: none;
    }

    .edit-panel {
        display: none;
        width: 50%;
        height: 800px;
        margin: auto;
        background-color: #f0f0f0;
        top: 0;
        bottom: 0;
        position: absolute;
        right: 0;
        left: 0;
        border-radius: 20px;
    }

    .dropzone {
        border: 2px dashed black;
        padding: 80px;
        margin: 10px !important;
        transition: all 0.18s;
    }
    
    .dropzone:hover {
        cursor: default !important;
    }

    .nav-item {
        width: 33.3%;
        text-align: center;
    }

    .nav-pills .nav-link.active {
        background-color: #e6e9ec !important;
        border-bottom: 3px solid white;
        color: #595454 !important;
    }

    .nav-pills {
        background-color: #e6e9ec;
    }

    .nav-pills .nav-link {
        border-radius: 0px !important;
        color: #595454 !important;
        font-weight: bold;
        font-size: 12px;
    }

    #margin *,
    #mobile-margin *,
    #padding *,
    #mobile-padding *,
    #text * {
        padding: 0;
    }

    input {
        padding: 7px 9px 8px !important;
    }

    .tab-content {
        padding: 0;
    }

    .image-thumbnail {
        width: -webkit-fill-available;
        width: -moz-available;
    }

    .container {
        max-width: 75%;
    }

    .tox-textarea {
        display: block;
    }

    #margin > div.dx-first-row.dx-last-row.dx-field-item.dx-col-1.dx-field-item-optional.dx-flex-layout {
        padding: 0 !important;
    }

    #gallery-source,
    #gridGallery-source,
    #horizontal-scroll-gallery-source {
        max-height: 400px;
    }

    h4 {
        margin-bottom: 0 !important;
    }

    .card-title {
        margin-bottom: 0.75rem;
        margin-top: 0.75rem;
    }

    .edit-title-icon {
        margin-bottom: auto;
        margin-left: 15px;
        cursor: pointer;
        display: none;
    }

    .page-card {
        opacity: 0.7;
        box-shadow: 0 1px 2px rgba(0,0,0,0.15);
        padding: 20px;
        border: 0.1px solid black;
        border-radius: 4px;
        background-color: #f3f3f39e;
        margin: 4px;
        width: 187px;
        cursor: pointer;
        transition: all 0.3s;
    }

    .page-card:hover {
        opacity: 1;
        box-shadow: 0px 0px 4px 2px rgba(0,0,0,0.18);
        transform: scale(1.03);
    }

    .widget-wrapper {
        border-radius: 3px;
        background-color: #fff;
        cursor: move;
        margin: 4px;
        position: relative;
        color: #556068;
        width: 47.8%;
    }

    .widget-wrapper:hover {
        box-shadow: 0px 0px 4px 2px rgba(0,0,0,0.18);
    }

    .widget-thumbnail {
        font-size: 2.5em !important;
    }

    .vertical-pdf-scroll-container {
        position: relative;
        margin: 15px;
        padding: 8px;
        width: 100%;
        overflow-y: scroll;
    }

    .horizontal-pdf-scroll {
        position: relative;
        margin: 15px;
        display: flex;
        overflow-x: scroll;
        padding: 8px;
    }

    .dx-layout-manager .dx-label-h-align .dx-field-item-content .dx-checkbox, .dx-layout-manager .dx-label-h-align .dx-field-item-content .dx-switch {
        float: right;
    }

    canvas {
        padding: 6px;
    }

    .vertical-pdf-scroll-container>canvas {
        max-width: 100%;
    }

    .canvas-pdf-scrollable {
        margin: 0 auto;
        display: block;
    }

    .switch {
        position: relative;
        display: inline-block;
        width: 60px;
        height: 34px;
    }

    .switch input {
        opacity: 0;
        width: 0;
        height: 0;
    }

    .slider {
        position: absolute;
        cursor: pointer;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background-color: #ccc;
        -webkit-transition: .4s;
        transition: .4s;
    }

    .slider:before {
        position: absolute;
        content: "";
        height: 26px;
        width: 26px;
        left: 4px;
        bottom: 4px;
        background-color: white;
        -webkit-transition: .4s;
        transition: .4s;
    }

    input:checked+.slider {
        background-color: #2196F3;
    }

    input:focus+.slider {
        box-shadow: 0 0 1px #2196F3;
    }

    input:checked+.slider:before {
        -webkit-transform: translateX(26px);
        -ms-transform: translateX(26px);
        transform: translateX(26px);
    }

    /* Rounded sliders */
    .slider.round {
        border-radius: 34px;
    }

    .slider.round:before {
        border-radius: 50%;
    }

    .page-options {
        margin-right: 0 !important;
        margin: auto;
        display: flex;
    }

    #prev-page,
    #next-page,
    #zoom-in,
    #zoom-out {
        background-color: transparent;
        border: none;
    }

    #zoom-in,
    #zoom-out {
        float: right;
    }

    .pdf-toolbar {
        background-color: #4B4B4B;
        border-radius: 20px;
    }

    .map-container-default {
        margin: auto;
        width: auto;
        height: 500px;
    }

    .web-page-container-default {
        width: auto;
        height: 100vh;
    }

    .video-container-default {
        width: auto;
        height: 600px;
    }

    #prev-page>.fas,
    #next-page>.fas,
    #zoom-in>.fas,
    #zoom-out>.fas {
        line-height: 2 !important;
        color: white;
    }

    .grid-gallery {
        display: flex;
        flex-wrap: wrap;
        justify-content: space-evenly;
        align-items: center;
    }

    .grid-gallery>.image-item:hover .actions-panel {
        opacity: 1;
    }

    .grid-gallery>.image-item {
        width: calc(25% - 16px);
        margin: 8px;
        position: relative;
    }

    .image-item>img {
        width: 100%;
        max-height: 450px;
        object-fit: contain;
        display: block;
    }

    .grid-gallery>.selectable {
        cursor: pointer;
    }

    .pdf-page-info {
        margin-left: 1rem;
        color: white;
    }

    .gallery {
        display: flex;
        flex-direction: row;
        height: 100%;
        overflow: hidden;
    }

    .gallery .gallery-container {
        display: flex;
        flex-direction: row;
        height: 100%;
        flex: 1 1 auto;
        padding: 24px 0;
        overflow-x: scroll;
        -ms-overflow-style: none;
        /* IE and Edge */
        scrollbar-width: none;
        /* Firefox */
    }

    .gallery .gallery-container .item-gallery-image {
        height: 100%;
        width: auto;
        margin: 0 3px;
        position: relative;
        display: block;
        cursor: pointer;
    }

    .gallery .gallery-container .item-gallery-image img {
        height: 100%;
        object-fit: contain;
        display: block;
    }

    .gallery .gallery-container .item-gallery-image:first-of-type {
        margin-left: 0;
    }

    .gallery .gallery-container .item-gallery-image:last-of-type {
        margin-right: 0;
    }

    .gallery .gallery-container .item-gallery-image:hover {
        transform: scale(1.1);
        z-index: 100;
    }

    .gallery .gallery-container .item-gallery-image:hover .actions-panel {
        opacity: 1;
    }

    .gallery .gallery-container .disable-scale:hover {
        transform: scale(1) !important;
        z-index: 0;
    }

    .gallery .gallery-container .disable-scale:hover .actions-panel {
        display: none;
    }

    .gallery .gallery-container::-webkit-scrollbar {
        display: none;
    }

    .gallery .navigation-arrow {
        height: 100%;
        display: flex;
        align-items: center;
        flex: 0 0 56px;
    }

    .gallery .navigation-arrow svg-icon {
        width: 40px;
        height: 40px;
        border-radius: 20px;
        margin: 8px;
        fill: white;
        padding: 4px;
        cursor: pointer;
        background-color: rgba(0, 0, 0, 0.7);
    }

    .gallery .navigation-arrow svg-icon:hover {
        background-color: rgba(0, 0, 0, 0.9);
    }

    .gallery .left-arrow {
        transform: rotateZ(180deg);
    }

    .horizontal-scroll-wrapper {
        position: absolute;
        display: flex;
        top: 0;
        left: 0;
        width: auto;
        margin: 0;
        overflow-y: auto;
        overflow-x: auto;
    }

    .left-icon {
        left: 0;
        position: absolute;
        top: 50%;
        margin-left: 20px;
    }

    .right-icon {
        right: 0;
        position: absolute;
        top: 50%;
        margin-right: 20px;
    }

    .left-icon,
    .right-icon {
        color: white;
        font-size: 2.33333em;
        z-index: 101;
    }

    .widget:hover {
        cursor: pointer;
    }

    .edit-icon {
        padding: 3px;
        display: none;
        width: fit-content;
        right: 0;
        left: 0;
        margin: auto;
        cursor: pointer;
    }

    .page-structure {
        padding-top: 10px;
        border: 1px dotted;
    }

    .structure {
        margin: 0px 10px;
        outline: 2px dotted #312d2d !important;
        transition: all 0.2s;
    }

    .widget {
        transition: all 0.2s;
    }

    .fade-scale {
      transform: scale(0);
      opacity: 0;
      -webkit-transition: all .50s linear;
      -o-transition: all .50s linear;
      transition: all .50s linear;
    }

    .fade-scale.in {
      opacity: 1;
      transform: scale(1);
    }

    .page {
        max-width: 108%;
        margin-right: 18% !important;
    }

    .draggable-widgets-list {
        height: 560px;
        transition: height 0.3s;
    }

    .dx-buttongroup-wrapper {
        width: 100% !important;
    }

    .row {
        margin: 0;
    }

    .edit-panel-container {}

    .edit-button-container {
        width: 20%;
        right: 0;
        left: 0;
        margin: auto;
    }

    a {
        color: #9a9a9a !important;
    }

    .nav-link.active {
        color: black !important;
    }

    #page {
        text-align: center;
    }

    input[type='checkbox'] {
        cursor: pointer;
    }

    textarea {
        display: none;
    }

    #history-container {
        margin: 0;
    }

    #history-container *, #structure-icon:hover {
        cursor: pointer;
    }

    #structure-icon:hover {
        transform: scale(1.2);
    }

    .dx-button {
        border-radius: 0;
    }

    .modal-dialog {
        margin: auto;
        position: absolute;
        top: 40%;
        left: 0;
        transform: translateY(-50%) !important;
        right: 0;
    }

    .modal-content {
        border-radius: 0px;
    }
    
    .form-select {
        display: block;
        width: 100%;
        padding: 0.375rem 2.25rem 0.375rem 0.75rem;
        -moz-padding-start: calc(0.75rem - 3px);
        font-size: 1rem;
        font-weight: 400;
        line-height: 1.5;
        color: #212529;
        background-color: #fff;
        background-image: url(data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 16 16'%3e%3cpath fill='none' stroke='%23343a40' stroke-linecap='round' stroke-linejoin='round' stroke-width='2' d='M2 5l6 6 6-6'/%3e%3c/svg%3e);
        background-repeat: no-repeat;
        background-position: right 0.75rem center;
        background-size: 16px 12px;
        border: 1px solid #ced4da;
        border-radius: 0.25rem;
        transition: border-color .15s ease-in-out, box-shadow .15s ease-in-out;
        -webkit-appearance: none;
        -moz-appearance: none;
        appearance: none;
    }

    #buttons {
        right: 0;
        padding: 0;
    }

    #filters:hover {
        cursor: pointer;
    }

    #info {
        display: flex;
        color: white;
        margin-right: 20px !important;
        margin: auto;
        vertical-align: middle;
        top: 0 !important;
        bottom: 0;
    }

    #info p {
        margin-top: revert;
    }

    .bg-dark {
        background-color: #e6e9ec !important;
    }

    .fa-xl {
        font-size: 1.1em;
    }

    .badge {
        display: table;
        margin-left: auto;
        margin-right: auto;
    }

    .dx-buttongroup-wrapper {
        background-color: #fff;
        width: 156px;
        margin-right: 2px;
        border-radius: 4px;
    }

    .dx-button-mode-outlined.dx-buttongroup-last-item.dx-button {
        border: none !important;
    }

    .dx-toast-content {
      min-width: 300px;
      max-width: 400px;
    }

    .dx-fa-button.dx-fa-button-main .dx-overlay-content, .dx-fa-button.dx-fa-button-main:hover {
        background-color: #3e9d36;
    }

    #loader-container {
        position: fixed;
        z-index: -1;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        margin: auto;
    }
    
    .iframe-overlay {
        position: absolute;
        width: fit-content !important;
        height: fit-content !important;
        inset: 0px;
        opacity: 0 !important;
        z-index: 2;
        margin: auto;
        display: block !important;
        width: 100% !important;
        height: 100% !important;
    }

    .edit-overlay {
        position: absolute;
        width: fit-content !important;
        height: fit-content !important;
        inset: 0px;
        opacity: 0 !important;
        z-index: 2;
        margin: auto;
        display: block !important;
    }

    .iframe-overlay i {
        font-size: 30px;
    }

    #create-page {
        margin: 0.5rem;
    }
    
    .newLanguageBtn {
        margin: auto;
        margin-right: 0;
        margin-bottom: 0;
    }
    
    .language-select {
        display: flex;
        margin: auto;
        margin-right: 0;
        margin-bottom: 0;
    }
    
    .language-option {
        padding: 7px;
        margin: 3px 3px 0px 0px;
        border-radius: 3px;
        border: 1px solid #999999;
        cursor: pointer;
    }
    
    .fa-xl {
        font-size: 1.6em;
    }

</style>

<body>
    <div id="loader-container">
        <div class="loader" id="loader"></div>
    </div>
    <div id="wrapper" class="page-wrapper chiller-theme toggled" style="height: 100%;">

        <nav id="navbar" class="navbar navbar-dark bg-dark">
            <i id="go-back" class="dx-icon-arrowleft" style="display: none; color: #000 !important; font-size: 30px; cursor: pointer; color: #9a9a9a !important; margin-right: 10px;"></i>
            <h5 id="page-title"></h5>
            <form id="search" value="" class="form-inline my-2 my-lg-0" style="width: 75%; display: flex; left: 0; right: 0; margin: auto;">
                <input id="search" class="form-control mr-sm-2" style="width: 80%;" type="search" placeholder="Search" required>
                <select value="all" class="form-select form-inline form-select-sm" style="width: 9%;" aria-label=".form-select-sm example" id="filter" required>
                  <option value="all" selected>Tutte</option>
                  <option value="1">Pagine</option>
                  <option value="0">Home</option>
                </select>
                <button id="create-page" type="button" class="btn btn-outline-secondary" data-toggle="modal" data-target=".bd-example-modal-lg">Crea Pagina</button>
                <div id="change-view"></div>
            </form>
            <div id="info" style="display: none;"><p style="margin-right: 0.4rem; color: black;">Stato: </p><p id="status"></p></div>
            <div id="history-container" style="display: block;">
